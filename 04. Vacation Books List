using System;

namespace Vacation_Books_List
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int pagesInBook = int.Parse(Console.ReadLine());
            int pagesForOneHour = int.Parse(Console.ReadLine());
            int days = int.Parse(Console.ReadLine());
            int result = pagesInBook / pagesForOneHour;
            int resultForDayHours = result / days;
            Console.WriteLine(resultForDayHours);
        }
    }
}
